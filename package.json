{
  "name": "front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "yarn clean-dist && yarn build:front && yarn start:back",
    "build": "concurrently \"yarn build:front\" \"yarn build:back\"",
    "test": "concurrently \"yarn test:back\" \"yarn test:front\"",
    "lint": "concurrently \"yarn lint:front\" \"yarn lint:front\"",
    "start:dev": "concurrently \"yarn start:back:dev\" \"yarn start:front\"",
    "start:front": "ng serve",
    "start:back": "yarn build:back && node dist",
    "start:back:dev": "nodemon --watch .env --watch server/src --exec babel-node server/src --source-maps",
    "build:front": "ng build",
    "build:back": "babel server/src -D --out-dir dist",
    "clean-dist": "rimraf dist",
    "e2e": "ng e2e",
    "lint:front": "ng lint",
    "lint:back": "eslint server/src test",
    "lint:back:fix": "eslint server/src server/test --fix",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "prettier": "prettier --write **/*.{js,json,yml,yaml,md}",
    "seed:database": "babel-node server/src/data/seed",
    "clean:database": "babel-node server/src/data/clean",
    "test:front": "ng test",
    "test:back": "cross-env NODE_ENV=test mocha --exit --recursive --require @babel/register server/test",
    "test:back:coverage": "cross-env NODE_ENV=test nyc --require @babel/register --reporter lcov --reporter text mocha --exit --recursive server/test"
  },
  "husky": {
    "hooks": {}
  },
  "lint-staged": {
    "*.js": [
      "eslint  --fix",
      "prettier --write",
      "git add"
    ],
    "*.{md,yml,yml,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "javascript",
    "node",
    "express",
    "es6",
    "api",
    "angular 8+",
    "jest",
    "docker",
    "material"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@hapi/boom": "^8.0.1",
    "@hapi/joi": "^16.1.8",
    "@sentry/node": "^5.10.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "bookshelf": "^1.0.1",
    "bookshelf-virtuals-plugin": "^0.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.0.0-beta.2",
    "dotenv-safe": "^6.1.0",
    "express": "^4.17.1",
    "express-validator": "^5.3.1",
    "helmet": "^3.21.2",
    "http-status-codes": "^1.4.0",
    "jest-config": "latest",
    "jsonwebtoken": "^9.0.0",
    "knex": "^0.20.4",
    "lodash": "^4.17.13",
    "mongoose": "^5.5.15",
    "mongoose-paginate-v2": "^1.3.0",
    "morgan": "^1.9.1",
    "nodemailer": "^6.2.1",
    "nodemailer-mailgun-transport": "^1.4.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.15.1",
    "request-ip": "^2.1.3",
    "rxjs": "~6.4.0",
    "serve-favicon": "^2.5.0",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-dist": "^3.24.3",
    "trim-request": "^1.0.6",
    "tslib": "^1.10.0",
    "uuid": "^3.3.2",
    "validator": "^11.0.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "^8.3.2",
    "@angular-devkit/build-angular": "~0.803.21",
    "@angular/cli": "~8.3.21",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
